{
  "name": "react-min-startup",
  "version": "1.0.0",
  "description": "react app startup with `webpack` `bable` `redux` `test` `antd` `echarts` with browserrouter",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && NODE_ENV=production webpack --config ./scripts/webpack.config.js",
    "start": "NODE_ENV=development webpack serve --config ./scripts/webpack.server.config.js",
    "clean": "rm -rf ./dist",
    "serve": "http-server ./dist/",
    "prepare": "husky install",
    "lint": "eslint . --ext .tsx,.ts -f table && stylelint '**/*.{less,css}' -f verbose"
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "eslint --fix -f table",
      "stylelint --fix -f verbose",
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-virtualized": "^9.21.10",
    "@types/react-window": "^1.8.2",
    "@types/redux-logger": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-loader": "^8.0.6",
    "css-loader": "^5.0.1",
    "eslint": "^7.29.0",
    "eslint-plugin-react": "^7.24.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "less-loader": "^10.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "react-refresh": "^0.10.0",
    "schema-utils": "^3.0.0",
    "style-loader": "^3.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "terser-webpack-plugin": "^5.1.4",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.4",
    "webpack": "5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@huse/boolean": "^1.0.4",
    "ahooks": "^2.10.6",
    "antd": "^4.16.5",
    "formik": "^2.2.9",
    "i": "^0.3.6",
    "immer": "^9.0.3",
    "lorem-ipsum": "^2.0.3",
    "react": "18.0.0-alpha-73ffce1b6-20210624",
    "react-dom": "18.0.0-alpha-73ffce1b6-20210624",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-virtualized": "^9.22.2",
    "react-window": "^1.8.6",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  }
}
