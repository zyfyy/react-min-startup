{
  "name": "react-min-startup",
  "version": "1.0.0",
  "description": "react app startup with `webpack` `bable` `redux` `test` `antd` `echarts` with browserrouter",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && NODE_ENV=production webpack --config ./scripts/webpack.config.js",
    "start": "cross-env NODE_ENV=development webpack serve --config ./scripts/webpack.server.config.js",
    "clean": "rm -rf ./dist",
    "serve": "http-server ./dist/",
    "prepare": "husky install",
    "lint": "eslint . --ext .tsx,.ts -f table && stylelint '**/*.{less,css}' -f verbose"
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "eslint --fix -f table",
      "stylelint --fix -f verbose",
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/node": "^16.10.4",
    "@types/react": "^17.0.29",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.19",
    "@types/react-router-dom": "^5.3.1",
    "@types/react-virtualized": "^9.21.13",
    "@types/react-window": "^1.8.5",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.1",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "less": "^4.1.2",
    "less-loader": "^10.1.0",
    "lint-staged": "^11.2.3",
    "postcss": "^8.3.9",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "react-refresh": "^0.10.0",
    "schema-utils": "^3.1.1",
    "style-loader": "^3.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "terser-webpack-plugin": "^5.2.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "5.48.0",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0",
    "workbox-webpack-plugin": "^6.3.0"
  },
  "dependencies": {
    "@huse/boolean": "^1.1.2",
    "ahooks": "^2.10.11",
    "antd": "^4.16.13",
    "formik": "^2.2.9",
    "i": "^0.3.7",
    "immer": "^9.0.6",
    "lorem-ipsum": "^2.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-use": "^17.3.1",
    "react-virtualized": "^9.22.3",
    "react-window": "^1.8.6",
    "redux": "^4.1.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  }
}
